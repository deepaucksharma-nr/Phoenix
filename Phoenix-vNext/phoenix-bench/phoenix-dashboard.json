{
  "dashboard": {
    "id": null,
    "title": "Phoenix-vNext Cardinality Optimization",
    "tags": [
      "phoenix",
      "cardinality",
      "optimization",
      "benchmarking"
    ],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Cardinality Overview",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "count(count by (__name__)({__name__=~\"phoenix_.*\", job=\"phoenix-metrics-generator\"}))",
            "legendFormat": "Total Phoenix Metrics",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short",
            "custom": {
              "displayMode": "list",
              "orientation": "horizontal"
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ],
            "fields": ""
          },
          "orientation": "horizontal",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 2,
        "title": "Pipeline Cardinality Comparison",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 6
        },
        "targets": [
          {
            "expr": "count(count by (__name__)({__name__=~\"phoenix_.*\", pipeline_id=\"full\"}))",
            "legendFormat": "Full Pipeline",
            "refId": "A"
          },
          {
            "expr": "count(count by (__name__)({__name__=~\"phoenix_.*\", pipeline_id=\"opt\"}))",
            "legendFormat": "Opt Pipeline",
            "refId": "B"
          },
          {
            "expr": "count(count by (__name__)({__name__=~\"phoenix_.*\", pipeline_id=\"ultra\"}))",
            "legendFormat": "Ultra Pipeline",
            "refId": "C"
          },
          {
            "expr": "count(count by (__name__)({__name__=~\"phoenix_.*\", pipeline_id=\"exp\"}))",
            "legendFormat": "Experimental Pipeline",
            "refId": "D"
          },
          {
            "expr": "count(count by (__name__)({__name__=~\"phoenix_.*\", pipeline_id=\"hybrid\"}))",
            "legendFormat": "Hybrid Pipeline",
            "refId": "E"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {
                "type": "linear"
              },
              "axisCenteredZero": false,
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "calcs": []
          }
        }
      },
      {
        "id": 3,
        "title": "Cost Reduction Analysis",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 6
        },
        "targets": [
          {
            "expr": "(1 - (count(count by (__name__)({__name__=~\"phoenix_.*\", pipeline_id=\"ultra\"})) / count(count by (__name__)({__name__=~\"phoenix_.*\", pipeline_id=\"full\"})))) * 100",
            "legendFormat": "Ultra Pipeline Reduction %",
            "refId": "A"
          },
          {
            "expr": "(1 - (count(count by (__name__)({__name__=~\"phoenix_.*\", pipeline_id=\"opt\"})) / count(count by (__name__)({__name__=~\"phoenix_.*\", pipeline_id=\"full\"})))) * 100",
            "legendFormat": "Opt Pipeline Reduction %",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 25
                },
                {
                  "color": "green",
                  "value": 50
                }
              ]
            },
            "unit": "percent",
            "custom": {
              "displayMode": "basic",
              "orientation": "horizontal"
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ],
            "fields": ""
          },
          "orientation": "horizontal",
          "textMode": "value_and_name",
          "colorMode": "background"
        }
      },
      {
        "id": 4,
        "title": "Phoenix Metrics by Type",
        "type": "piechart",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 14
        },
        "targets": [
          {
            "expr": "count by (__name__)({__name__=~\"phoenix_http_.*\"})",
            "legendFormat": "HTTP Metrics",
            "refId": "A"
          },
          {
            "expr": "count by (__name__)({__name__=~\"phoenix_system_.*\"})",
            "legendFormat": "System Metrics",
            "refId": "B"
          },
          {
            "expr": "count by (__name__)({__name__=~\"phoenix_user_.*\"})",
            "legendFormat": "User Metrics",
            "refId": "C"
          },
          {
            "expr": "count by (__name__)({__name__=~\"phoenix_business_.*\"})",
            "legendFormat": "Business Metrics",
            "refId": "D"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ],
            "fields": ""
          },
          "pieType": "pie",
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "displayMode": "list",
            "placement": "right",
            "calcs": []
          },
          "displayLabels": [
            "name",
            "value"
          ]
        }
      },
      {
        "id": 5,
        "title": "Current Control Signal Status",
        "type": "table",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 14
        },
        "targets": [
          {
            "expr": "phoenix_observer_mode",
            "legendFormat": "Observer Mode",
            "refId": "A",
            "format": "table"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true
              },
              "indexByName": {},
              "renameByName": {
                "mode": "Current Mode",
                "optimization_level": "Optimization Level",
                "ts_count": "Time Series Count"
              }
            }
          }
        ],
        "options": {
          "showHeader": true,
          "sortBy": []
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Real-time Metrics Generation Rate",
        "type": "timeseries",
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 22
        },
        "targets": [
          {
            "expr": "rate(phoenix_http_requests_total[1m])",
            "legendFormat": "HTTP Requests/sec",
            "refId": "A"
          },
          {
            "expr": "rate(phoenix_user_activity_total[1m])",
            "legendFormat": "User Activities/sec",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {
                "type": "linear"
              },
              "axisCenteredZero": false,
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "calcs": []
          }
        }
      }
    ]
  },
  "overwrite": true
}